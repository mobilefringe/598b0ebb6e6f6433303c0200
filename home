<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var repo = getRepoDetailsByName("About Banner");
            if (repo != undefined) {
                var banner_images = repo.images;
                console.log(banner_images)
                $.each(banner_images, function(key, val) {
                    var banner_img_url = val.photo_url_abs;
                    $("<div><div class='bottom image_medium' style='background-image: url(" + banner_img_url + ");'></div></div>").appendTo("#banner_container");
                });
            }
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-about.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            var feature_items = getFeatureList();
            renderFeatureItems('#feature_container','#feature_template', feature_items);
            show_content();
            
            $('.slick-slider').slick({
                arrows: false,
                dots: true,
                autoplay: true,
                autoplaySpeed: 5000  
            });
        }
        loadMallDataCached(renderAll); 
    });
</script>