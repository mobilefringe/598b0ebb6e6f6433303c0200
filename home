<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var banner_images = getBanners();
            if (banner_images != undefined) {
                console.log(banner_images)
                $.each(banner_images, function(key, val) {
                    var banner_img_url = val.image_url;
                    var banner_message = val.name;
                    $("<div>\
                        <div class='bottom image_tall' style='background-image: url(" + banner_img_url + ");'>\
                            <div class='slider_gradient'></div>\
                            <div class='slide-caption'>\
                                <div class='text-wrapper'>\
                                    <h1><p>" + banner_message + "</p></h1>\
                                </div>\
                            </div>\
                        </div>\
                    </div>").appendTo("#banner_container");
                });
            }
            
            $('#myModal').modal('show');
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-about.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            var feature_items = getFeatureList();
            renderFeatureItems('#feature_container','#feature_template', feature_items);
            
            show_content();
            
            $('.slick-slider').slick({
                arrows: false,
                dots: true,
                autoplay: true,
                autoplaySpeed: 5000  
            });
        }
        loadMallDataCached(renderAll); 
    });
</script>