<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            // var repo = getRepoDetailsByName("Tourism Banner");
            // if (repo != undefined) {
            //     var banner = getImageURL(repo.images[0].photo_url);
            //     $(".page_header").css({backgroundImage: "url(" + banner + ")"});
            // }
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-about.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            show_content();
            
            $.validate();
        }
        loadMallDataCached(renderAll); 
        
        $('#contact_form').submit(function(e) {
            values = [];
            values = JSON.stringify($('#contact_form').serializeArray());
            e.preventDefault();
            $.ajax({
                url : "/api/v1/contact_us",
                type: "POST",
                data : {authenticity_token: '<%=form_authenticity_token%>' ,
                form_data: values },
                success: function(data, textStatus, jqXHR) {
                    $('#send_contact_success').fadeIn();
                    $('#contact_form').find("input[type=text], input[type=email], textarea").val("");
                    $('html, body').animate({scrollTop : 0},800);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    $('#send_contact_error').fadeIn();
                    $('html, body').animate({scrollTop : 0},800);
                }
            });
        });
    });
</script>