<div class="paused">Paused</div>
<div class="slick-slider" id="banner_container"></div>

<div id="maincontent" class="container">
    <div class="sixteen clearfix content">
	    <div class="four">
	        <div class="left-sidebar top-padding">
                <a href="/leasing" class="active">Overview</a>
                <a href="/leasing/retail-portfolio" class="">Retail Portfolio</a>
                <a href="/leasing/office-portfolio" class="">Office Portfolio</a>
                <a href="/leasing/other-portfolio" class="">Other Portfolio</a>
            </div>	
        </div>	
	    <div class="twelve pagecontent">	
			<h1>Leasing Portfolio</h1>
		    <div id="page_content"></div>
	    </div>		
    </div>
</div>
<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var repo = getRepoDetailsByName("About Banner");
            if (repo != undefined) {
                var banner_images = repo.images;
                $.each(banner_images, function(key, val) {
                    var banner_img_url = val.photo_url_abs;
                    $("<div><div class='bottom image_medium' style='background-image: url(" + banner_img_url + ");'></div></div>").appendTo("#banner_container");
                });
            }
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-leasing.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            show_content();
            
            $('.slick-slider').slick({
                arrows: false,
                dots: true,
                autoplay: true,
                autoplaySpeed: 5000  
            });
        }
        loadMallDataCached(renderAll); 
    });
</script>