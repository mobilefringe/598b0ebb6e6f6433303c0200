<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            // var repo = getRepoDetailsByName("Tourism Banner");
            // if (repo != undefined) {
            //     var banner = getImageURL(repo.images[0].photo_url);
            //     $(".page_header").css({backgroundImage: "url(" + banner + ")"});
            // }
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-leasing.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            show_content();
        }
        loadMallDataCached(renderAll); 
    });
</script>