<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var repo = getRepoDetailsByName("About Banner");
            console.log(repo);
            if (repo != undefined) {
                var banner_images = repo.images;
                console.log(banner_images);
                $.each(banner_images, function(key, val) {
                    var container = $(".slick-slider");
                    // var gallery_slug = $(val).attr('source');
                    // gallery = getFashionBySlug(gallery_slug).fashion_images;
                    renderBanner(container, "#banner_template", banner_images);
                });
            }
            
            var prefix = get_prefix();
            var pages_json = prefix + "/pages/20vic-about.json"
            $.getJSON(pages_json).done(function(data) {
                $('#page_content').html(data.body);
            }).fail(function(jqXHR) {
                if (jqXHR.status == 404) {
                    $("#404_msg").fadeIn("fast");
                }
            }); 
            
            show_content();
        }
        loadMallDataCached(renderAll); 
    });
</script>